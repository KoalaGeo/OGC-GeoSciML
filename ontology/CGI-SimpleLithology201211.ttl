#################################################################
#    Object Properties
#################################################################

###  http://resource.geosciml.org/classifier/cgi/lithOntology/ChemCat
CGI_Lith:ChemCat rdf:type owl:ObjectProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain CGI_Lith:rock_material ;
                 rdfs:range CGI_Lith:AnyChemCat ;
                 rdfs:comment "property that specifies one or more chemical categories for an earth material. Chemical category assignment requires a chemical analysis" ;
                 rdfs:label "chemical category" .


###  http://resource.geosciml.org/classifier/cgi/lithOntology/ConsolDegree
CGI_Lith:ConsolDegree rdf:type owl:ObjectProperty ,
                               owl:FunctionalProperty ;
                      rdfs:domain CGI_Lith:rock_material ;
                      rdfs:range CGI_Lith:AnyConsolDegree ;
                      rdfs:label "consolidation degree" .


###  http://resource.geosciml.org/classifier/cgi/lithOntology/Constituents
CGI_Lith:Constituents rdf:type owl:ObjectProperty ;
                      rdfs:domain CGI_Lith:rock_material ;
                      rdfs:range CGI_Lith:AnyConstituent ;
                      dc:description "property links to terms that are implemented by GeoSciML CompoundMaterial.constituent.ConstituentPart elements. The property value vocabulary specifies mineralogic composition like location in Streckeisen (1978) QAPF space, mafic mineral content (color index),  NADM metamorphic (2004) mineralogic composition categories, or sandstone QFL composition. The ConstituentPart elements represent collections of particles that have the same material composition or particleType. The material may be any kind of EarthMaterial (mineral, rock, fluid in pores). ParticleTypes are used to specify generic kinds of particle constituents like intraclasts, bioclasts, pyroclastic particle, etc." ;
                      rdfs:label "constituent part" .


###  http://resource.geosciml.org/classifier/cgi/lithOntology/FabricType
CGI_Lith:FabricType rdf:type owl:ObjectProperty ,
                             owl:FunctionalProperty ;
                    rdfs:domain CGI_Lith:rock_material ;
                    rdfs:range CGI_Lith:AnyFabricType ;
                    rdfs:label "fabric type" .


###  http://resource.geosciml.org/classifier/cgi/lithOntology/GeneticCateg
CGI_Lith:GeneticCateg rdf:type owl:ObjectProperty ,
                               owl:FunctionalProperty ;
                      rdfs:domain CGI_Lith:rock_material ;
                      rdfs:range CGI_Lith:AnyGeneticCateg ;
                      rdfs:label "genetic category" .


###  http://resource.geosciml.org/classifier/cgi/lithOntology/GrainSize
CGI_Lith:GrainSize rdf:type owl:ObjectProperty ,
                            owl:FunctionalProperty ;
                   rdfs:domain CGI_Lith:rock_material ;
                   rdfs:range CGI_Lith:AnyGrainSize ;
                   rdfs:label "grain size category" .


###  http://resource.geosciml.org/classifier/cgi/lithOntology/MetamorphicFacies
CGI_Lith:MetamorphicFacies rdf:type owl:ObjectProperty .


###  http://resource.geosciml.org/classifier/cgi/lithOntology/MetamorphicGrade
CGI_Lith:MetamorphicGrade rdf:type owl:ObjectProperty ;
                          rdfs:domain CGI_Lith:rock ;
                          rdfs:range CGI_Lith:AnyMetaGrade ;
                          rdfs:label "metamorphic grade" .


###  http://resource.geosciml.org/classifier/cgi/lithOntology/ParticleType
CGI_Lith:ParticleType rdf:type owl:ObjectProperty ;
                      rdfs:domain CGI_Lith:rock_material ;
                      rdfs:range CGI_Lith:AnyParticleType ;
                      rdfs:label "particle type" .


###  http://resource.geosciml.org/classifier/cgi/lithOntology/Protolith
CGI_Lith:Protolith rdf:type owl:ObjectProperty ;
                   rdfs:domain CGI_Lith:composite_transformed_genesis_material ;
                   rdfs:range CGI_Lith:rock ;
                   rdfs:label "protolith" .


###  http://resource.geosciml.org/classifier/cgi/lithOntology/Shape
CGI_Lith:Shape rdf:type owl:ObjectProperty ;
               rdfs:domain CGI_Lith:rock_material ;
               rdfs:range CGI_Lith:AnyShape ;
               rdfs:label "particle shape category" .


###  http://resource.geosciml.org/classifier/cgi/lithOntology/Sorting
CGI_Lith:Sorting rdf:type owl:ObjectProperty ;
                 rdfs:domain CGI_Lith:rock_material ;
                 rdfs:range CGI_Lith:AnySorting ;
                 rdfs:label "particle sorting category" .


