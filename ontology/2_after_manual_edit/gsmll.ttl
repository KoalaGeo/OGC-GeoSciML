###  http://geosciml.org/ont/gsmll#parentBorehole
gsmll:parentBorehole rdf:type owl:ObjectProperty ;
                     rdfs:range gsmll:BoreholeView ;
                     skos:definition "URI referring to the unique ID of a parent borehole (eg, parent well of a sidetrack wellbore)"@en .


###  http://geosciml.org/ont/gsmll#positionalAccuracy
gsmll:positionalAccuracy rdf:type owl:ObjectProperty ;
                         rdfs:range mdq:PositionalAccuracy ;
                         skos:definition "An estimate of the accuracy of the location "@en .


###  http://geosciml.org/ont/gsmll#startPoint
gsmll:startPoint rdf:type owl:ObjectProperty ;
                 rdfs:range skos:Concept ;
                 skos:definition "Indicates the position relative to the ground surface where the borehole commenced (eg, open pit floor or wall, underground, natural land surface, sea floor)"@en .


###  http://geosciml.org/ont/gsmll#status
gsmll:status rdf:type owl:ObjectProperty ;
             rdfs:range skos:Concept ;
             skos:definition "The present status of the borehole (eg, abandoned, completed, proposed, suspended)"@en .


