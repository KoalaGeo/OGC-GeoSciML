                    rdfs:label "Any particle sorting" .


###  http://resource.geosciml.org/classifier/cgi/lithOntology/CalciumSulfateMineralogy
CGI_Lith:CalciumSulfateMineralogy rdf:type owl:Class ;
                                  rdfs:subClassOf CGI_Lith:nonCarbonateSaltMineralogy ;
                                  dc:description "consists of more than 50 percent gypsum or anhydrite" .


###  http://resource.geosciml.org/classifier/cgi/lithOntology/F_GT0_LT10
CGI_Lith:F_GT0_LT10 rdf:type owl:Class ;
                    rdfs:subClassOf CGI_Lith:M_LT90 ;
                    owl:disjointWith CGI_Lith:F_GT10_LT60 ,
                                     CGI_Lith:F_GT60 ;
                    dc:description "Feldspathoid minerals form between 0 and 10 percent of QAPF fraction" .


###  http://resource.geosciml.org/classifier/cgi/lithOntology/F_GT10_LT60
CGI_Lith:F_GT10_LT60 rdf:type owl:Class ;
                     rdfs:subClassOf CGI_Lith:M_LT90 ;
                     owl:disjointWith CGI_Lith:F_GT60 ,
                                      CGI_Lith:amphibolitic ;
                     dc:description "Feldspathoid minerals form between 10 and 60 percent of QAPF fraction" .


###  http://resource.geosciml.org/classifier/cgi/lithOntology/F_GT60
CGI_Lith:F_GT60 rdf:type owl:Class ;
                rdfs:subClassOf CGI_Lith:M_LT90 ;
                owl:disjointWith CGI_Lith:amphibolitic ,
                                 CGI_Lith:hybridSedimentaryConstituents ;
                dc:description "Feldspathoid minerals form greater than  60 percent of QAPF fraction" .


###  http://resource.geosciml.org/classifier/cgi/lithOntology/F_GT60_LT90
CGI_Lith:F_GT60_LT90 rdf:type owl:Class ;
                     owl:equivalentClass [ owl:intersectionOf ( CGI_Lith:F_GT60
                                                                [ rdf:type owl:Class ;
                                                                  owl:complementOf CGI_Lith:F_GT90
                                                                ]
                                                              ) ;
                                           rdf:type owl:Class
                                         ] ;
                     rdfs:subClassOf CGI_Lith:F_GT60 ;
                     owl:disjointWith CGI_Lith:F_GT90 .


###  http://resource.geosciml.org/classifier/cgi/lithOntology/F_GT90
CGI_Lith:F_GT90 rdf:type owl:Class ;
                owl:equivalentClass CGI_Lith:QAPF15c ;
                rdfs:subClassOf CGI_Lith:F_GT60 ;
                dc:description "Feldspathoid minerals form greater than 90 percent of QAPF fraction" .


###  http://resource.geosciml.org/classifier/cgi/lithOntology/M_GT10_LT90
CGI_Lith:M_GT10_LT90 rdf:type owl:Class ;
                     rdfs:subClassOf CGI_Lith:M_LT90 ;
                     owl:disjointWith CGI_Lith:M_LTE_10 ;
                     dc:description "Mafic minerals form between 10 and 90 percent of rock." ;
                     rdfs:label "Between 10 and 90 percent modal mafic minerals" .


###  http://resource.geosciml.org/classifier/cgi/lithOntology/M_LT90
CGI_Lith:M_LT90 rdf:type owl:Class ;
                rdfs:subClassOf CGI_Lith:AnyConstituent ;
                owl:disjointWith CGI_Lith:calcFerruginousSilicate ,
                                 CGI_Lith:exotic ,
                                 CGI_Lith:glass_dominant ,
                                 CGI_Lith:ultramafic ;
                rdfs:comment "Mafic minerals form less than 90 percent of rock. Category for normal-composition igneous rocks, dominated by quartz, feldspar, and feldspathoid minerals, with compositions determined by fields in the Streckeisen 1978 QAPF triangles." ;
                rdfs:label "Normal composition igneous rock" .


###  http://resource.geosciml.org/classifier/cgi/lithOntology/M_LTE_10
CGI_Lith:M_LTE_10 rdf:type owl:Class ;
                  rdfs:subClassOf CGI_Lith:M_LT90 ;
                  owl:disjointWith CGI_Lith:amphibolitic ,
                                   CGI_Lith:mafic ;
                  dc:description "Mafic minerals form less than 10 percent of rock." ;
                  rdfs:label "Less than 10 percent modal mafic minerals" .


###  http://resource.geosciml.org/classifier/cgi/lithOntology/P_A_GT10_LT35
CGI_Lith:P_A_GT10_LT35 rdf:type owl:Class ;
